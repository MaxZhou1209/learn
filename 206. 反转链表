# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution(object):
    def reverseList(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        index=head
        pre=None
        
        while index!=None:
            temp=index.next
            index.next=pre         
            pre=index
            index=temp
                      
        return pre
