class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        dict={}
        for i in range(len(nums)):
            dict[nums[i]] = i
            i+=1

        for i in range(len(nums)):
            number = target-nums[i]
            if dict.get(number, False):
                #print(dict[nums[i]])
                #print(dict[target-nums[i]])
                if i != dict[number]:
                    list=[i,dict[number]]
                    return list
            i+=1
