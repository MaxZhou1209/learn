# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def kthToLast(self, head, k):
        """
        :type head: ListNode
        :type k: int
        :rtype: int
        """
        node=head
        n=0
        while node!=None:
            node=node.next
            n+=1
        
        node=head
        for i in range(n-k):
            node=node.next
            i+=1
        print(node)
        return node.val
