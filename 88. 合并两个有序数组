class Solution(object):
    def merge(self, nums1, m, nums2, n):
        """
        :type nums1: List[int]
        :type m: int
        :type nums2: List[int]
        :type n: int
        :rtype: None Do not return anything, modify nums1 in-place instead.
        """
        sortt=[]
        p1=p2=0
        print(p1)
        print(p2)
        helper1=nums1[:m]
        helper2=nums2[:n]
        print(helper1)
        print(helper2)
        while p1<m or p2<n:
            if p1==m:
                sortt.append(helper2[p2])
                p2+=1
            elif p2==n:
                sortt.append(helper1[p1])
                p1+=1
            elif helper1[p1]<helper2[p2] :
                sortt.append(helper1[p1])
                p1+=1
            else:
                sortt.append(helper2[p2])
                p2+=1

        print(sortt)     
        nums1[:] = sortt
        print(nums1)
